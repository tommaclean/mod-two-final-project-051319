<h1>Welcome Page</h1>

<h2> Hi, <%= @dj.name %> (<%= @dj.username %>)!</h2>



<h3>Your Playlists:</h3>

<% if @dj.playlists %>
<ul>
  <% @dj.playlists.each do |playlist| %>
  <li>
    <%= link_to playlist.name, edit_playlist_path %>
  </li>
  <% end %>
</ul>
<% end %>

<h3>Your Liked Playlists: </h3>
<% if @liked_playlists %>
<ul>
  <% @liked_playlists.each do |playlist| %>
  <li>
    <%= link_to playlist.name, playlist_path %>
  </li>
  <% end %>
</ul>
<% end %>

<%= link_to "Make a New Playlist", new_playlist_path %>
